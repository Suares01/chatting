    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.5",
    "axios-auth-refresh": "^3.3.6",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
